{
  "name": "medusa-2.0-boilerplate-backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "ib": "init-backend",
    "start": "init-backend && cd .medusa/server && medusa db:migrate && medusa start --verbose",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@alpha-solutions/medusa-image-alt": "^1.1.0",
    "@devx-commerce/strapi": "^0.0.12",
    "@linearcommerce/product-custom-attributes": "^0.0.6",
    "@medusajs/admin-sdk": "2.10.3",
    "@medusajs/cli": "2.10.3",
    "@medusajs/core-flows": "2.10.3",
    "@medusajs/dashboard": "2.10.3",
    "@medusajs/draft-order": "2.10.3",
    "@medusajs/framework": "2.10.3",
    "@medusajs/icons": "2.10.3",
    "@medusajs/js-sdk": "2.10.3",
    "@medusajs/medusa": "2.10.3",
    "@medusajs/notification-sendgrid": "2.10.3",
    "@medusajs/payment-stripe": "2.10.3",
    "@medusajs/types": "2.10.3",
    "@medusajs/ui": "^4.0.23",
    "@medusajs/utils": "2.10.3",
    "@medusajs/workflow-engine-redis": "2.10.3",
    "@medusajs/workflows-sdk": "2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "@strapi/client": "^1.5.0",
    "@variablevic/mollie-payments-medusa": "^0.0.13",
    "jsonwebtoken": "^9.0.2",
    "medusa-variant-images": "^1.1.4",
    "minio": "^8.0.6",
    "pdfmake": "^0.2.20",
    "resend": "^6.4.1"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.3",
    "@react-email/components": "^0.5.7",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@tanstack/react-query": "^5.64.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.1",
    "react-router-dom": "^6.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "ulid": "^3.0.1",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53",
    "zod": "3.25.76"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}